{
  "name": "@motorcycle/dom",
  "version": "0.6.0",
  "description": "Snabbdom driver for motorcycle.js",
  "main": "lib/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint src/",
    "mocha": "mocha --compilers js:babel-core/register",
    "test": "npm run eslint && npm run test-browser",
    "compile-lib": "babel src/ -d lib/",
    "pretest-browser": "browserify test/browser/index.js -t babelify -o test/browser/page/tests-bundle.js",
    "test-browser": "testem",
    "posttest-browser": "rm test/browser/page/tests-bundle.js",
    "pretest-browser-perf": "browserify test/browser/perf/index.js -t babelify -o test/browser/page/tests-bundle.js",
    "test-browser-perf": "testem",
    "posttest-browser-perf": "rm test/browser/page/tests-bundle.js",
    "prepublish": "npm run test && npm run compile-lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motorcyclejs/motorcycle-dom.git"
  },
  "keywords": [
    "Cyclejs",
    "Cycle",
    "Most",
    "Mostjs",
    "Reactive",
    "Framework"
  ],
  "author": "Tylor Steinberger <tlsteinberger167@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/motorcyclejs/motorcycle-dom/issues"
  },
  "homepage": "https://github.com/motorcyclejs/motorcycle-dom#readme",
  "peerDependencies": {
    "@motorcycle/core": "*"
  },
  "devDependencies": {
    "@motorcycle/core": "^0.1.5",
    "assert": "^1.3.0",
    "babel-cli": "^6.1.4",
    "babel-core": "^6.1.4",
    "babel-eslint": "^4.1.5",
    "babel-preset-es2015": "^6.1.4",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "1.0.x",
    "eslint-config-cycle": "^3.1.0",
    "eslint-plugin-cycle": "^1.0.2",
    "eslint-plugin-no-class": "^0.1.0",
    "mocha": "^2.3.3",
    "most": "^0.16.0",
    "testem": "^0.9.8",
    "wallabify": "0.0.10"
  },
  "dependencies": {
    "fast.js": "^0.1.1",
    "hyperscript-helpers": "^2.0.2",
    "matches-selector": "^1.0.0",
    "snabbdom": "^0.2.7"
  }
}
